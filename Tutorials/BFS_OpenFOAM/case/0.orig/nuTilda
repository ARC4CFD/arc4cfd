FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1e-05;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet
    {
        type            zeroGradient;
    }

    "(wall_hor|wall_ver|wall_in)"
    {
        type            zeroGradient;
    }
    top
    {
        type            symmetryPlane;
    }

    "(front|back)"
    {
        type            cyclic;
    }

}


// ************************************************************************* //
